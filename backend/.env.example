# VendoCasa Backend Configuration
# Copy this file to .env and fill in your values

# PostgreSQL connection (with PostGIS)
DATABASE_URL=postgresql+psycopg://vendocasa:vendocasa@localhost:5432/vendocasa

# Data directory for OMI zip files
DATA_DIR=./data

# Google Geocoding API key (optional, Nominatim is used as primary geocoder)
GOOGLE_GEOCODING_API_KEY=

# CORS allowed origins (comma-separated)
CORS_ORIGINS=http://localhost:5173

# Anthropic API key (required for AI chat agent)
# Get yours at https://console.anthropic.com/
ANTHROPIC_API_KEY=
